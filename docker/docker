docker images
docker rmi "image-name"
docker rmi -f $(docker images -aq)

docker ps -a
docker run "image-name"
docker restart "container-name"
docker stop "container-name"
docker rm "container-name"

docker build -t "image-name:tag" .
docker tag "image-name:tag" "username/new-image-name:tag"
docker login -u "username" -p "password"
docker push "username/new-image-name:tag"
docker pull "username/image-nam:tag"

sudo rm -r shared

docker context ls
docker context create ecs "context-name"
docker context create ecs-local context "context-name"
docker context use "context-name"

docker system df
docker system prune -a